%Course
figure()
plot(rad2deg(chi)); hold on; plot(rad2deg(chi_c))
legend('\chi', '\chi^c')
title('Course'); xlabel('time (sec)'); ylabel('course (deg)')

%Input
figure()
plot(rad2deg(delta_a))
legend('\delta_a')
title('Aeleron Input'); xlabel('time (sec)'); ylabel('aeileron deflection (deg)')

%Yaw
figure()
plot(rad2deg(phi)); hold on; plot(phi_c)
legend('\phi', '\phi^c')
title('Yaw'); xlabel('time (sec)'); ylabel('Yaw (deg)')